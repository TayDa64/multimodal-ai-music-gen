{
  "name": "multimodal-ai-music-gen",
  "version": "1.0.0",
  "description": "Multimodal AI Music Generator - JUCE app with Python backend",
  "scripts": {
    "build": "cd juce/build && cmake --build . --config Release",
    "build:debug": "cd juce/build && cmake --build . --config Debug",
    "rebuild": "cd juce/build && cmake --build . --config Release --clean-first",
    "configure": "cd juce/build && cmake ..",
    "start": "start \"\" \"juce\\build\\MultimodalMusicGen_artefacts\\Release\\AI Music Generator.exe\"",
    "start:server": "python -m multimodal_gen.server --verbose",
    "stop": "taskkill /IM \"AI Music Generator.exe\" /F & taskkill /IM python.exe /F",
    "test": "python -m pytest test_suite.py -v",
    "test:quick": "python -m pytest test_suite.py -v -x",
    "clean": "cd juce/build && cmake --build . --target clean"
  },
  "keywords": [
    "music",
    "ai",
    "midi",
    "juce",
    "osc"
  ],
  "author": "TayDa64",
  "license": "MIT"
}
